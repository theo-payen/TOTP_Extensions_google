{
    "manifest_version": 3,
    "name": "Mon extension",
    "version": "1.0",
    "description": "Description de mon extension",
    "action": {
        "default_popup": "popup.html",
        "default_icon": {
            "128": "img/icons.png"
        }
    },
    "permissions": [
        "storage"
    ],
    "host_permissions": ["<all_urls>"]
}